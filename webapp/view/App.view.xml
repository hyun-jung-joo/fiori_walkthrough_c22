<mvc:View
    controllerName="ui5.walkthrough_c22.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
>
    <!-- 양 옆 공간을 두기 위해 Shell 사용 -->
    <Shell>
        <App class="myAppDemoWT">
            <pages>
                <Page title="{i18n>homePageTitle}">
                    <content>
                        <!-- HelloPanel.view.xml 을 중첩시킨다 -->
                        <mvc:XMLView
                            viewName="ui5.walkthrough_c22.view.HelloPanel"
                        />
                        <!-- InvoiceList.view.xml 을 중첩 -->
                        <mvc:XMLView
                            viewName="ui5.walkthrough_c22.view.InvoiceList"
                        />
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
